  "dependencies": {
    "@twurple/api": "^7.3.0",
    "@twurple/auth": "^7.3.0",
    "@typegoose/typegoose": "^13.1.0",
    "canvas": "^3.1.0",
    "cheerio": "^1.0.0",
    "dbd-soft-ui": "^1.7.40-beta.1",
    "discord.js": "^14.25.1",
    "dotenv": "^16.5.0",
    "lodash": "^4.17.21",
    "mongoose": "^9.1.5",
    "ms": "^2.1.3",
    "node-cron": "^3.0.3",
    "pretty-ms": "^9.2.0",
