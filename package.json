    "@types/lodash": "^4.17.16",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^22.18.6",
    "@types/node-cron": "^3.0.11",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.2.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.5",
